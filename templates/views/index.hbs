{{!-- landing content start --}}
<div id="carouselExampleControls" class="carousel slide mx-5" data-bs-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img width="200" height="300" style="border:1px solid" src="https://images.unsplash.com/photo-1546587348-d12660c30c50?ixid=MnwxMjA3fDB8MHxzZWFyY2h8NXx8bmF0dXJhbHxlbnwwfHwwfHw%3D&ixlib=rb-1.2.1&w=1000&q=80" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img width="200" height="300" style="border:1px solid" src="https://wonderfulengineering.com/wp-content/uploads/2016/01/nature-wallpapers-38.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img width="200" height="300" style="border:1px solid" src="https://wallpapercave.com/wp/wp2904877.jpg" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>

<div class="card-group mx-5">
